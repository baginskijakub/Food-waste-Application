<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.LoginViewController">
   <children>
      <Button layoutX="41.0" layoutY="35.0" mnemonicParsing="false" onAction="#backButton" text="&lt;" AnchorPane.leftAnchor="40.0" AnchorPane.topAnchor="40.0">
         <font>
            <Font name="Arial" size="18.0" />
         </font>
      </Button>
      <TextField fx:id="usernameField" layoutX="364.0" layoutY="241.0">
         <font>
            <Font name="Arial" size="24.0" />
         </font>
      </TextField>
      <PasswordField fx:id="passwordField" layoutX="363.0" layoutY="316.0" onAction="#clickLogin" prefHeight="43.0" prefWidth="301.0" />
      <Label layoutX="368.0" layoutY="225.0" text="Enter username">
         <font>
            <Font name="Arial" size="13.0" />
         </font>
      </Label>
      <Label layoutX="366.0" layoutY="299.0" text="Enter password">
         <font>
            <Font name="Arial" size="13.0" />
         </font>
      </Label>
      <Button layoutX="477.0" layoutY="401.0" mnemonicParsing="false" onAction="#clickLogin" text="Login">
         <font>
            <Font name="Arial" size="18.0" />
         </font>
      </Button>
      <Label fx:id="errorLabel" alignment="CENTER" contentDisplay="CENTER" layoutX="481.0" layoutY="368.0" text="errorLabel" textAlignment="CENTER" textFill="RED">
         <font>
            <Font name="Arial" size="13.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
